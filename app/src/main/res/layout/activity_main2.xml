<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/main"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#AF13DCD1"
tools:context=".MainActivity2">


    <ImageView
        android:id="@+id/bottomLogo"
        android:layout_width="507dp"
        android:layout_height="359dp"
        android:layout_marginTop="107dp"
        android:alpha="0.2"
        android:contentDescription="@string/md_count_track_logo"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.656"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints,VisualLintBounds,ImageContrastCheck" />

    <ImageView
        android:id="@+id/masLogo"
        android:layout_width="166dp"
        android:layout_height="75dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:contentDescription="@string/mas_logo"
        android:src="@drawable/maslog"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ImageContrastCheck" />

<!-- App Title -->
    <TextView
        android:id="@+id/title"
        android:layout_width="284dp"
        android:layout_height="117dp"
        android:autofillHints=""
        android:background="#6F1D96BF"
        android:backgroundTintMode="src_in"
        android:shadowColor="#29C4CC"
        android:text="@string/md"
        android:textColor="#1228A4"
        android:textColorHighlight="#270FA9"
        android:textSize="50sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.143"
        tools:ignore="TextSizeCheck" />


<Spinner
    android:id="@+id/spinnerShift"
    android:layout_width="260dp"
    android:layout_height="48dp"
    android:padding="10dp"
    android:spinnerMode="dropdown"
    android:background="#43142CE2"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.503"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.499" />

<EditText
    android:id="@+id/epfNumber"
    android:layout_width="269dp"
    android:layout_height="48dp"
    android:layout_marginStart="24dp"
    android:layout_marginEnd="24dp"
    android:autofillHints=""
    android:background="#43142CE2"
    android:backgroundTint="@color/black"
    android:hint="@string/enter_your_epf_number"
    android:inputType="number"
    android:padding="10dp"
    android:textColorHint="#795548"
    android:textColorLink="@color/black"
    android:textStyle="bold"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.553"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.655"
    tools:ignore="DuplicateIds,TextContrastCheck,TextFields" />

<Button
    android:id="@+id/enterBtn"
    android:layout_width="260dp"
    android:layout_height="48dp"
    android:layout_marginTop="20dp"
    android:backgroundTint="#003366"
    android:text="ENTER"
    android:textColor="#ffffff"
    android:textStyle="bold"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.503"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.901"
    tools:ignore="HardcodedText" />


</androidx.constraintlayout.widget.ConstraintLayout>
